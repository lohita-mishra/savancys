<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="com.savancys">
	<namespace>sys</namespace>
	<entity local-service="true" name="ContactUs" table="ContactUs" remote-service="true" uuid="true">

		<!-- PK fields -->
		<column name="contactId" primary="true" type="long" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->
		<column name="inquiryType" type="String" />
		<column name="firstName" type="String" />
		<column name="lastName" type="String" />
		<column name="phoneNumber" type="String" />
		<column name="email" type="String" />
		<column name="companyName" type="String" />
		<column name="country" type="String" />
		<column name="additionalInfo" type="String" />

		<!-- Order -->
		<order by="asc">
			<order-column name="contactId" />
		</order>
		
	</entity>
</service-builder>